{
  "name": "web-api-migration",
  "version": "1.0.0",
  "description": "migration script for knex",
  "private": true,
  "scripts": {
    "migrate:make": "knex --knexfile knexfile.js migrate:make -- -x js",
    "migrate:up": "knex --knexfile knexfile.js migrate:up",
    "migrate:down": "knex --knexfile knexfile.js migrate:down",
    "migrate:latest": "knex --knexfile knexfile.js migrate:latest",
    "seed:run":"knex seed:run"
  },
  "author": "Andy",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "knex": "^3.1.0",
    "pg": "^8.12.0"
  }
}
